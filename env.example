# REQUIRED: Must be 'true' to run scripts (safety gate)
HL_IS_TESTNET=true

# Optional overrides (if you set them they MUST contain the substring 'testnet')
HL_API_BASE_URL=https://api.hyperliquid.testnet.example
HL_WS_URL=wss://ws.hyperliquid.testnet.example

# Optional: set to a testnet private key if you want to send signed orders (ONLY testnet keys)
# If empty, placeOrder will run in dry-run mode (builds payload but does not send).
WALLET_PRIVATE_KEY=

# Optional: dry-run override: if set to "false" and WALLET_PRIVATE_KEY is set,
# placeOrder will actually send. Default behavior is safe (dry-run).
DRY_RUN=true

# Optional: example address for info queries
TEST_USER_ADDRESS=0x0000000000000000000000000000000000000000



